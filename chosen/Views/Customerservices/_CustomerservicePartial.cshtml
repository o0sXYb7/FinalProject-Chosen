@model IEnumerable<chosen.Models.Customerservice>

  @foreach (var item in Model)
{
    <div class="container px-4 px-lg-5 my-5">
        <div class="row">
            <div class="col"> 
                <div class="tabs">
                    <div class="tab">
                                <input type="checkbox" id="flush-heading@(item.CustomerserviceId)">
                                <label class="tab-label larger-text" for="flush-heading@(item.CustomerserviceId)">@Html.DisplayFor(modelItem => item.QuestionTitle)</label>
                        <div class="tab-content larger-text">
                            @Html.DisplayFor(modelItem =>item.AnswerTitle)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}




